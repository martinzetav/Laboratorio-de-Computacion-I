Algoritmo actividad1
	Dimension array[5]
	Escribir 'Ingrese 5 numeros'
	Para i<-0 Hasta 4 Hacer
		Leer array[i]
	FinPara
	Escribir 'Los numeros que contiene el arreglo son:'
	Escribir '[' Sin Saltar
	Para i<-0 Hasta 3 Hacer
		Escribir array[i],', ' Sin Saltar
	FinPara
	Escribir array[4],']'
FinAlgoritmo
